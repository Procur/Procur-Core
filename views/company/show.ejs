<% if (loggedin === true ) { %>
  <%- partial('../partials/company-profile-header-logged-in') %>
<% } %>
<% if (loggedin === false) { %>
  <%- partial('../partials/company-profile-header-logged-out') %>
<% } %>

<% if (company.buyer === true && company.supplier === true) { %>
  <% if (company.primaryMode === "buyer") { %>
    <%- partial('../company/buyer-profile') %>
    <% } %>
  <% if (company.primaryMode === "supplier") { %>
    <%- partial('../company/supplier-profile') %>
  <% } %>
<% } %>
<% if (company.buyer === true && company.supplier === false) { %>
  <%- partial('../company/buyer-profile') %>
<% } %>
<% if (company.supplier === true && company.buyer === false) { %>
  <%- partial('../company/supplier-profile') %>
<% } %>

<%- partial('../partials/dashboard-footer.ejs') %>
