<div id="regWizard">    
	<div class="container">
		<div class="row">
			<div class="col-sm-12">
				<h1>Complete Your Registration</h1>
			</div>
			<form action="/buyer/create" method="post">
				<div class="col-sm-6 col-sm-offset-3 topBox">
					<h1>Your Buyer Profile</h1>
				</div>
				<div class="col-sm-6 col-sm-offset-3 fieldBox">
					<section class="buyerWizard">
						<div class="row">
							<div class="col-sm-6">
								<h4>Logo Image</h4>
								<div><input class="breakForm" type="file" name="logo"></div>
								<h4>DBA (Doing Business As) Name</h4>
								<div><input class="breakForm" type="text" name="dba" placeholder="Alternate Company Name"></div>
								
								<h4>Type of Company*</h4>
								<section id="typeOfCompany">
									<div class="styledSelect breakForm">
										<select name="typeOfCompany">
											<%- partial('../partials/dropdowns/buyer-type') %>
										</select>
									</div>
								</section>

								<h4>Language(s)*</h4>
								<div class="styledSelect">
									<select name="language1">
										<%- partial('../partials/dropdowns/language') %>
									</select>
								</div>
								<div class="styledSelect breakForm">
									<select name="language2">
										<%- partial('../partials/dropdowns/language') %>
									</select>
								</div>

								<h4>Nearest Port City*</h4>
								<section id="nearestPort">
									<div><input type="text" name="nearestPortCity" placeholder="City"></div>
									<div><input type="text" name="nearestPortProvince" placeholder="State / Province"></div>
									<div><input class="breakForm" type="text" name="nearestPortCountry" placeholder="Country"></div>
								</section>

								<h4>Other Locations</h4>
								<section id="otherLocation1">
									<div><input type="text" name="otherLocation1Name" placeholder="Location Title"></div>
									<div class="styledSelect">
										<select name="type">
											<%- partial('../partials/dropdowns/other-location-type') %>
										</select>
									</div>
									<div class="styledSelect">
										<select name="country">
											<option value="1">Select Country</option>
											<option value="2">2</option>
										</select>
									</div>
									<div class="styledSelect">
										<select name="province">
											<option value="1">Select State/Province</option>
											<option value="2">2</option>
										</select>
									</div>
									<div><input type="text" name="otherLocation1City" placeholder="City"></div>
								</section>



								<!--<section id="otherLocation2">
									<div><input type="text" name="otherLocation2Name" placeholder="Name"></div>
									<div><input type="text" name="otherLocation2Type" placeholder="Type"></div>
									<div><input type="text" name="otherLocation2City" placeholder="City"></div>
									<div><input type="text" name="otherLocation2Province" placeholder="State / Province"></div>
									<div><input type="text" name="otherLocation2Country" placeholder="Country"></div>
								</section>

								<section id="otherLocation3">
									<div><input type="text" name="otherLocation3Name" placeholder="Name"></div>
									<div><input type="text" name="otherLocation3Type" placeholder="Type"></div>
									<div><input type="text" name="otherLocation3City" placeholder="City"></div>
									<div><input type="text" name="otherLocation3Province" placeholder="State / Province"></div>
									<div><input type="text" name="otherLocation3Country" placeholder="Country"></div>
								</section>-->

								
							</div>
							<div class="col-sm-6">
								

								<h4>Accepted Delivery Terms</h4>
								<section id="acceptedDeliveryTerms">
									<div class="row breakForm">
										<div class="col-sm-6">
											<input class="checkbox" type="checkbox" name="deliveryTerms" value="EXW"><div class="checkboxtext">EXW</div><br>
											<input class="checkbox" type="checkbox" name="deliveryTerms" value="FCA"><div class="checkboxtext">FCA</div><br>
											<input class="checkbox" type="checkbox" name="deliveryTerms" value="CPT"><div class="checkboxtext">CPT</div><br>
											<input class="checkbox" type="checkbox" name="deliveryTerms" value="CIP"><div class="checkboxtext">CIP</div><br>
											<input class="checkbox" type="checkbox" name="deliveryTerms" value="DAT"><div class="checkboxtext">DAT</div><br>
											<input class="checkbox" type="checkbox" name="deliveryTerms" value="DAP"><div class="checkboxtext">DAP</div><br>
										</div>
										<div class="col-sm-6">
											<input class="checkbox" type="checkbox" name="deliveryTerms" value="DDP"><div class="checkboxtext">DDP</div><br>
											<input class="checkbox" type="checkbox" name="deliveryTerms" value="FAS"><div class="checkboxtext">FAS</div><br>
											<input class="checkbox" type="checkbox" name="deliveryTerms" value="FOB"><div class="checkboxtext">FOB</div><br>
											<input class="checkbox" type="checkbox" name="deliveryTerms" value="CFR"><div class="checkboxtext">CFR</div><br>
											<input class="checkbox" type="checkbox" name="deliveryTerms" value="CIF"><div class="checkboxtext">CIF</div><br>
											<input class="checkbox" type="checkbox" name="deliveryTerms" value="Custom"><div class="checkboxtext">Custom</div><br>
										</div>
									</div>
								</section>

								<h4>Accepted Currencies</h4>
								<section id="acceptedCurrencies">
									<div class="row breakForm">
										<div class="col-sm-6">
											<input class="checkbox" type="checkbox" name="deliveryTerms" value="USD"><div class="checkboxtext">USD</div><br>
											<input class="checkbox" type="checkbox" name="deliveryTerms" value="EUR"><div class="checkboxtext">EUR</div><br>
											<input class="checkbox" type="checkbox" name="deliveryTerms" value="JPY"><div class="checkboxtext">JPY</div><br>
											<input class="checkbox" type="checkbox" name="deliveryTerms" value="GBP"><div class="checkboxtext">GBP</div><br>
											<input class="checkbox" type="checkbox" name="deliveryTerms" value="AUD"><div class="checkboxtext">AUD</div><br>
											<input class="checkbox" type="checkbox" name="deliveryTerms" value="CHF"><div class="checkboxtext">CHF</div><br>
											<input class="checkbox" type="checkbox" name="deliveryTerms" value="CAD"><div class="checkboxtext">CAD</div><br>
										</div>
										<div class="col-sm-6">
											<input class="checkbox" type="checkbox" name="deliveryTerms" value="MXN"><div class="checkboxtext">MXN</div><br>
											<input class="checkbox" type="checkbox" name="deliveryTerms" value="CNY"><div class="checkboxtext">CNY</div><br>
											<input class="checkbox" type="checkbox" name="deliveryTerms" value="NZD"><div class="checkboxtext">NZD</div><br>
											<input class="checkbox" type="checkbox" name="deliveryTerms" value="SEK"><div class="checkboxtext">SEK</div><br>
											<input class="checkbox" type="checkbox" name="deliveryTerms" value="RUB"><div class="checkboxtext">RUB</div><br>
											<input class="checkbox" type="checkbox" name="deliveryTerms" value="HKD"><div class="checkboxtext">HKD</div><br>
											<input class="checkbox" type="checkbox" name="deliveryTerms" value="SGD"><div class="checkboxtext">SGD</div><br>
										</div>
									</div>
								</section>

								<h4>Accepted Payment Terms</h4>
								<section id="acceptedPaymentTerms">
									<div class="row breakForm">
										<div class="col-sm-6">
											<input class="checkbox" type="checkbox" name="deliveryTerms" value="MoneyGram"><div class="checkboxtext">MoneyGram</div><br>
											<input class="checkbox" type="checkbox" name="deliveryTerms" value="Western Union"><div class="checkboxtext">Western Union</div><br>
											<input class="checkbox" type="checkbox" name="deliveryTerms" value="T/T"><div class="checkboxtext">T/T</div><br>
											<input class="checkbox" type="checkbox" name="deliveryTerms" value="Credit Card"><div class="checkboxtext">Credit Card</div><br>
										</div>
										<div class="col-sm-6">
											<input class="checkbox" type="checkbox" name="deliveryTerms" value="Escrow"><div class="checkboxtext">Escrow</div><br>
											<input class="checkbox" type="checkbox" name="deliveryTerms" value="PayPal"><div class="checkboxtext">PayPal</div><br>
											<input class="checkbox" type="checkbox" name="deliveryTerms" value="D/P D/A"><div class="checkboxtext">D/P D/A</div><br>
											<input class="checkbox" type="checkbox" name="deliveryTerms" value="Cash"><div class="checkboxtext">Cash</div><br>
										</div>
									</div>
								</section>

								<h4>Your Supplier Preferences</h4>
								<section id="preferredSupplierType" class="styledSelect">
									<select name="preferredSupplierType">
										<%- partial('../partials/dropdowns/pref-supplier-type') %>
									</select>
								</section>
								<section id="preferredSupplierLocation1" class="styledSelect">
									<select name="preferredSupplierLocation1Country">
										<option value="1">Preferred Supplier Location</option>
										<option value="2">2</option>
									</select>
								</section>
								<section id="preferredSupplierLanguage1" class="styledSelect">
									<select name="preferredSupplierLanguage1">
										<%- partial('../partials/dropdowns/pref-supplier-language') %>
									</select>
								</section>

								
								
							</div>
						</div>
					</section>
				</div>
				<div class="row">
                    <div class="col-sm-6 col-sm-offset-3">
                        <div><button class="continueButton" type="submit">Continue&nbsp;&nbsp; <i class="fa fa-arrow-right"></i></button></div>
                    </div>
                </div>
			</form>
		</div>
	</div>
</div>

