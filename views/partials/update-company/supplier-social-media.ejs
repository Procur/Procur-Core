<div id="socialOutletsSupplier">
  <h4>Relevant Links</h4>
  <form action="/supplier/update/socialoutlets" method="post" id="supplierSocialOutletsForm">
    <div class="formWrapper">
      <div class="row">
        <div class="col-sm-6">

          <h3>Social Media Pages</h3>
          <h5>Facebook</h5>
          <% if (supplier.facebook === "undefined" || supplier.facebook === undefined) { %>
            <div class="input-group"><div class="input-group-addon">facebook.com/<input type="text" name="facebook" placeholder="Facebook"></div></div> <% } else { %>
            <div class="input-group"><div class="input-group-addon">facebook.com/<input type="text" name="facebook" value="<%= supplier.facebook %>" placeholder="Facebook"></div></div>
          <% } %>

          <h5>Twitter</h5>
          <% if (supplier.twitter === "undefined" || supplier.twitter === undefined) { %>
            <div class="input-group"><div class="input-group-addon">twitter.com/<input type="text" name="twitter" placeholder="Twitter"></div></div>
            <% } else { %>
            <div class="input-group"><div class="input-group-addon">twitter.com/<input type="text" name="twitter" value="<%= supplier.twitter %>" placeholder="Twitter"></div></div>
          <% } %>

          <h5>Google+</h5>
          <% if (supplier.google === "undefined" || supplier.google === undefined) { %>
            <div class="input-group"><div class="input-group-addon">plus.google.com/+<input type="text" name="google" placeholder="Google"></div></div>
            <% } else { %>
            <div class="input-group"><div class="input-group-addon">plus.google.com/+<input type="text" name="google" value="<%= supplier.google %>" placeholder="Google"></div></div>
          <% } %>

          <h5>LinkedIn</h5>
          <% if (supplier.linkedin === "undefined" || supplier.linkedin === undefined) { %>
            <div class="input-group"><div class="input-group-addon">linkedin.com/company/<input type="text" name="linkedin" placeholder="Linkedin"></div></div>
            <% } else { %>
            <div class="input-group"><div class="input-group-addon">linkedin.com/company/<input type="text" name="linkedin" value="<%= supplier.linkedin %>" placeholder="Linkedin"></div></div>
          <% } %>

          <h5>Pinterest</h5>
          <% if (supplier.pinterest === "undefined" || supplier.pinterest === undefined) { %>
            <div class="input-group"><div class="input-group-addon">pinterest.com/<input type="text" name="pinterest" placeholder="Pinterest"></div></div>
            <% } else { %>
            <div class="input-group"><div class="input-group-addon">pinterest.com/<input type="text" name="pinterest" value="<%= supplier.pinterest %>" placeholder="Pinterest"></div></div>
          <% } %>

          <h5>Instagram</h5>
          <% if (supplier.instagram === "undefined" || supplier.instagram === undefined) { %>
            <div class="input-group"><div class="input-group-addon">instagram.com/<input type="text" name="instagram" placeholder="Instagram"></div></div>
            <% } else { %>
            <div class="input-group"><div class="input-group-addon">instagram.com/<input type="text" name="instagram" value="<%= supplier.instagram %>" placeholder="Instagram"></div></div>
          <% } %>


          <h5>Tumblr</h5>
          <% if (supplier.tumblr === "undefined" || supplier.tumblr === undefined) { %>
            <div class="input-group" id="tumblrWeirdo"><div class="input-group-addon"><input id="tumblr-input" type="text" name="tumblr" placeholder="Tumblr">.tumblr.com</div></div>
            <% } else { %>
            <div class="input-group" id="tumblrWeirdo"><div class="input-group-addon"><input type="text" name="tumblr" value="<%= supplier.tumblr %>" placeholder="Tumblr">.tumblr.com</div></div>
          <% } %>

        </div>
        <!-- End First Column -->
        <div class="col-sm-6">

          <h3>Other Relevant Links</h3>

          <h5>Link 1</h5>
          <% if (supplier.relevantLinkTitle && supplier.relevantLinkTitle[0]) { %>
            <div class="relevantlink"><input type="text" name="linktitle" id="linktitle0" value="<%= supplier.relevantLinkTitle[0] %>" placeholder="Title"/></div>
          <% } else { %>
            <div class="relevantlink"><input type="text" name="linktitle" id="linktitle0" placeholder="Title"/></div>
          <% } %>
          <% if (supplier.relevantLinkUrl && supplier.relevantLinkUrl[0]) { %>
            <div class="relevantlink"><input type="text" name="linkurl" id="linkurl0" value="<%= supplier.relevantLinkUrl[0] %>" placeholder="http://www.mycompany.org"/></div>
          <% } else { %>
            <div class="relevantlink"><input type="text" name="linkurl" id="linkurl0" placeholder="http://www.mycompany.org"/></div>
          <% } %>

          <h5>Link 2</h5>
          <% if (supplier.relevantLinkTitle && supplier.relevantLinkTitle[1]) { %>
            <div class="relevantlink"><input type="text" name="linktitle" id="linktitle1" value="<%= supplier.relevantLinkTitle[1] %>" placeholder="Title"/></div>
          <% } else { %>
            <div class="relevantlink"><input type="text" name="linktitle" id="linktitle1" placeholder="Title"/></div>
          <% } %>
          <% if (supplier.relevantLinkUrl && supplier.relevantLinkUrl[1]) { %>
            <div class="relevantlink"><input type="text" name="linkurl" id="linkurl1" value="<%= supplier.relevantLinkUrl[1] %>" placeholder="http://www.mycompany.org"/></div>
          <% } else { %>
            <div class="relevantlink"><input type="text" name="linkurl" id="linkurl1" placeholder="http://www.mycompany.org"/></div>
          <% } %>

          <h5>Link 3</h5>
          <% if (supplier.relevantLinkTitle && supplier.relevantLinkTitle[2]) { %>
            <div class="relevantlink"><input type="text" name="linktitle" id="linktitle2" value="<%= supplier.relevantLinkTitle[2] %>" placeholder="Title"/></div>
          <% } else { %>
            <div class="relevantlink"><input type="text" name="linktitle" id="linktitle2" placeholder="Title"/></div>
          <% } %>
          <% if (supplier.relevantLinkUrl && supplier.relevantLinkUrl[2]) { %>
            <div class="relevantlink"><input type="text" name="linkurl" id="linkurl2" value="<%= supplier.relevantLinkUrl[2] %>" placeholder="http://www.mycompany.org"/></div>
          <% } else { %>
            <div class="relevantlink"><input type="text" name="linkurl" id="linkurl2" placeholder="http://www.mycompany.org"/></div>
          <% } %>

        </div>
        <!-- End Second Column -->
      </div><!-- END row -->
    </div><!-- END .formWrapper -->
    <div class="row"><button class="continueButton" type="submit">Save&nbsp;&nbsp; <i class="fa fa-check"></i></button></div>
  </form>
</div><!-- END #socialOutletsSupplier -->
